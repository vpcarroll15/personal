[run]
source = music
omit =
    */tests.py
    */test_*.py
    */migrations/*
    */__init__.py
    */apps.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
